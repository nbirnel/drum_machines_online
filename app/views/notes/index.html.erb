<h1>Listing notes</h1>

<table>
  <thead>
    <tr>
      <th>C3</th>
      <th>Asa yamaha</th>
      <th>Key num</th>
      <th>Freq</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @notes.each do |note| %>
      <tr>
        <td><%= note.c3 %></td>
        <td><%= note.asa_yamaha %></td>
        <td><%= note.key_num %></td>
        <td><%= note.freq %></td>
        <td><%= link_to 'Show', note %></td>
        <td><%= link_to 'Edit', edit_note_path(note) %></td>
        <td><%= link_to 'Destroy', note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Note', new_note_path %>
