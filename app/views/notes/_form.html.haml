= form_for(@note) do |f|
  - if @note.errors.any?
    #error_explanation
      %h2
        = pluralize(@note.errors.count, "error")
        prohibited this note from being saved:
      %ul
        - @note.errors.full_messages.each do |message|
          %li= message
  .field
    = f.label :c3
    %br/
    = f.text_field :c3
  .field
    = f.label :asa_yamaha
    %br/
    = f.text_field :asa_yamaha
  .field
    = f.label :name
    %br/
    = f.number_field :name
  .field
    = f.label :freq
    %br/
    = f.text_field :freq
  .field
    = f.label :sound
    %br/
    = f.text_field :sound
  .actions
    = f.submit
